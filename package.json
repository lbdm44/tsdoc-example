    "url": "https://github.com/lbdm44/tsdoc-example/issues"
  },
  "homepage": "https://github.com/lbdm44/tsdoc-example#readme",
  "volta": {
    "node": "16.13.2"
  },
  "devDependencies": {
    "@microsoft/api-documenter": "^7.15.0",
    "@microsoft/api-extractor": "^7.19.4",
    "husky": "^7.0.4",
    "lint-staged": "^12.2.0",
    "prettier": "2.5.1",
    "scripty": "^2.0.0",
    "typescript": "^4.5.4"
  },
  "lint-staged": {
    "**/*.{js,ts}": "prettier --write"
