{
  "name": "tsdoc-example",
  "version": "0.0.1",
  "description": "Working through examples using ts-doc",
  "main": "lib/index.js",
  "typings": "lib/index.d.ts",
  "scripts": {
    "api-extractor": "api-extractor",
    "api-documenter": "api-documenter",
